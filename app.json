{
  "expo": {
    "name": "my-app",
    "description": "A PWA using Expo Web",
    "slug": "my-app",
    "privacy": "public",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "icon": "./assets/images/icon.png",
      "splash": {
        "image": "./assets/images/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "bundleIdentifier": "com.myapp",
      "appStoreUrl": "app store url"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "androidPackage": "package",
      "playStoreUrl": "play store url"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png",
      "barStyle": "black-translucent",
      "backgroundColor": "#ffffff",
      "description": "A PWA using Expo Web",
      "display": "standalone",
      "lang": "en",
      "name": "my-app",
      "shortName": "my-app",
      "orientation": "portrait",
      "scope": "/",
      "manifest": {
        "display": "standalone",
        "orientation": "portrait",
        "themeColor": "#ffffff"
      },
      "themeColor": "#ffffff",
      "icon": "./assets/images/icon.png",
      "icons": {
        "16": "./assets/images/icon.png",
        "32": "./assets/images/icon.png",
        "48": "./assets/images/icon.png",
        "128": "./assets/images/icon.png"
      },
      "splash": {
        "image": "./assets/images/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "relatedApplications": [
        {
          "platform": "itunes",
          "url": "app store url",
          "id": "iOS bundle identifier"
        },
        {
          "platform": "play",
          "url": "play store url",
          "id": "android package"
        }
      ],
      "preferRelatedApplications": true
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
